{
    "presets": [
        "es2015", 
        "react"
    ],
    "plugins": [
        "add-module-exports",
        "transform-runtime",
        ["module-resolver", {
        "root": ["./"],
        "alias": {
            "src": "./app/scripts/",
            "test": "./test/",
            "client": "./app/scripts/client",
            "server": "./app/scripts/server"
            }
        }]
  ]
}
